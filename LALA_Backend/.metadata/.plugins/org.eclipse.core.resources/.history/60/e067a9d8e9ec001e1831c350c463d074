package A_Day1;

public class A_Day1 {
	//Static = 객체를 생성하지 않고 사용할 수 있는 메소드
	
	public static int sum(int n, int m) { //sum 메소드
		return n+m;
	}
	
	public static void main(String args[]) { //메인 메소드
		
		/*
		int a = 0;
		double d = 0.5;
		float f = 0;
		boolean b = true;
		char c = 'c';
		String S = "abcd";
		
		var v = 0;
		var V = 'c';
		*/
		
		int a = 5;
		int b = 3;
		
		int s;
		s = sum(a,b);
		
		//printf("%d \n",s);
		//System.out.println(s); // 개행 포함 출력
		//System.out.print(s); // 단일 출력
		
		char c = 'c'; // -> 문자 
		int x = 1; //숫자 
		
		System.out.println((char)c + x); //C에서는 출력 X
		
		
	}
}
